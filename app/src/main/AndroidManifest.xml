<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.cricketapp">
    <!-- Include required permissions for Google Mobile Ads to run. -->
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher_cricapp"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_cricapp_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <meta-data
                android:name="com.google.android.gms.ads.APPLICATION_ID"
                android:value="ca-app-pub-4777915220636073~8205537049"/>
        <activity
                android:name=".matchhistory"
                android:label="@string/title_activity_matchhistory">
        </activity>

        <activity
                android:name=".feedback"
                android:label="Feedback">
        </activity>
        <activity android:name=".details">
        </activity>
        <activity android:name=".aboutus">
        </activity>
        <activity
                android:name=".MainMenu"
                android:label="@string/title_activity_main_menu"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>